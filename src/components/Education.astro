---
import EducationItem from '../components/EducationTimeLine.astro';

const EDUCATION = [
    {
        startDate: '',
        endDate: 'October 2024',
        title: 'Computer Programming',
        company:'Niagara College Canada',
        description: 'Develop expertise in creating mobile applications, analyzing big data, and designing interactive multimedia solutions.',
       // link: '#'
    }
    
]
---
<ol class="relative mt-16">
    {
        EDUCATION.map((education) =>(
            <li class="">
                <EducationItem {...education}/>
            </li>
        ))
    }
</ol>


